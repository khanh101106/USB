a=int(input());c='';set=set()
def dfs(b):
    global c
    for i in ["1","6","8","9"]:
        if b==a:
            if solve(c):
                if c not in set:print(c);set.add(c)
        else:
            c+=i;dfs(b+1);c=c[:-1]
def solve(str,str2=''):
    for i in str[::-1]:
        if i=="9":str2+="6"
        elif i=="6":str2+="9"
        else:str2+=i
    if str==str2:return True
dfs(0)
